{
	"Format": "1.29.0",
	"ConfigSchema": {
		"Buildings": {
			"AllowValues": "farm, outdoors, town, warehouse",
			"AllowMultiple": true,
			"AllowBlank": true,
			"Default": "farm, outdoors, town, warehouse",
			"Description": "Enable changes to specified buildings.",
			"Section": "Buildings"},
		"Gold Clock": {
			"AllowValues": "true, false",
			"Default": "true",
			"Description": "Toggle changes to the gold clock farm building.",
			"Section": "Buildings"}
	},
	"DynamicTokens": [
		{
			"Name": "SeasonOverride",
			"Value": "{{season}}"
		},
		{
			"Name": "SeasonOverride",
			"Value": "Spring",
			"When": { "Query: NOT {{IsOutdoors}} OR {{LocationName |contains=Desert}}": true}
		},
		{
			"Name": "SeasonOverride",
			"Value": "Summer",
			"When": { "LocationName": "IslandEast, IslandNorth, IslandSouth, IslandSouthEast, IslandWest" }
		},
		{
			"Name": "FarmLayout",
			"Value": "{{FarmType}}"
		},
		{
			"Name": "FarmLayout",
			"Value": "JefGrizli.RedrawPelicanTownC/PetBowlShadow",
			"When": { "JefGrizli.RedrawPelicanTownC/PetBowlShadow": "Beach, Standard, FourCorners" }
		},
		{
			"Name": "FarmLayout",
			"Value": "void",
			"When": {
				"FarmType": "Standard",
				"HasMod": "flashshifter.GrandpasFarm, flashshifter.immersivefarm2remastered"
			}
		}
	],
